<table>
    name  = config0                         
    title = Configuration               
    <field>
        name       = field0             
        alias      = RecordInterval     
        title      = Record Interval    
        byteofs    = 0                  
        decpl      = 0                  
        type       = u32 
        units      = sec  
        min        = 1               
        default    = 60                      
    </field>
    <field>
        name       = field1            
        alias      = TransmissionInterval
        title      = Transmission Interval
        byteofs    = 4                  
        decpl      = 0
        type       = u32
        units      = min
        min        = 1
        default    = 60
        vscale     = 0.01666666667      
    </field>
    <field>
        name       = field2             
        alias      = TransmissionMode
        title      = Transmission Mode
        byteofs    = 8                  
        decpl      = 0
        type       = u8
        default    = 0
        editmask   = 0=Intervall;1=Wakeup;2=Online 
    </field>
</table>

